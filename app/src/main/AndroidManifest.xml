<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tuling.wallethdemo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <application
        android:name="com.tuling.wallethdemo.li.le.wallethdemo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tuling.wallethdemo.li.le.wallethdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tuling.wallethdemo.li.le.wallethdemo.ImportKeystore"/>
        <activity android:name="com.tuling.wallethdemo.li.le.wallethdemo.ImportPrivateKey"/>
        <activity android:name="com.tuling.wallethdemo.li.le.wallethdemo.SendActivity"/>
        <activity android:name="com.tuling.wallethdemo.li.le.wallethdemo.GenerateWallet"/>
        <activity android:name="com.tuling.wallethdemo.li.le.wallethdemo.SwitchWalletActivity"/>
    </application>

</manifest>